{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production",
    "watch-insights": "cd nova-components/Insights && npm run watch",
    "build-insights": "cd nova-components/Insights && npm run dev",
    "build-insights-prod": "cd nova-components/Insights && npm run prod",
    "prepare": "husky install"
  },
  "devDependencies": {
    "axios": "^0.21",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "laravel-mix": "^6.0.6",
    "lint-staged": "^13.2.2",
    "lodash": "^4.17.19",
    "postcss": "^8.1.14"
  }
}
