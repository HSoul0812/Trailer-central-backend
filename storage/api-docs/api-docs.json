{
    "openapi": "3.0.0",
    "info": {
        "title": "TC API",
        "contact": {
            "name": "Trailercentral",
            "email": "alberto@trailercentral.com"
        },
        "version": "1.0"
    },
    "paths": {
        "/interactions/send-email": {
            "post": {
                "tags": [
                    "interactions"
                ],
                "summary": "Send interaction email",
                "description": "",
                "operationId": "sendEmail",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "properties": {
                                    "user_id": {
                                        "description": "User Authentication ID.",
                                        "type": "string"
                                    },
                                    "lead_id": {
                                        "description": "Lead ID.",
                                        "type": "string"
                                    },
                                    "message_id": {
                                        "description": "Message ID.",
                                        "type": "string"
                                    },
                                    "subject": {
                                        "description": "Email subject.",
                                        "type": "string"
                                    },
                                    "body": {
                                        "description": "Email body.",
                                        "type": "string"
                                    },
                                    "filename": {
                                        "description": "Email attach file.",
                                        "type": "array",
                                        "items": {
                                            "type": "string",
                                            "format": "binary"
                                        }
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Email sent successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "description": "The response success",
                                            "type": "bool"
                                        },
                                        "message": {
                                            "description": "The response message",
                                            "type": "string"
                                        }
                                    },
                                    "type": "object",
                                    "example": {
                                        "success": true,
                                        "message": "Email sent successfully"
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "User not found | Lead not found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "description": "The response error",
                                            "type": "bool"
                                        },
                                        "message": {
                                            "description": "The response message",
                                            "type": "string"
                                        }
                                    },
                                    "type": "object",
                                    "example": {
                                        "error": true,
                                        "message": "User not found"
                                    }
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Throwable message",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "description": "The response success",
                                            "type": "bool"
                                        },
                                        "message": {
                                            "description": "The response message",
                                            "type": "string"
                                        }
                                    },
                                    "type": "object",
                                    "example": {
                                        "error": true,
                                        "message": "Throwable message"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/parts": {
            "get": {
                "operationId": "App\\Http\\Controllers\\v1\\Parts\\PartsController::index",
                "responses": {
                    "200": {
                        "description": "Returns part data",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "400": {
                        "description": "Error: Bad request."
                    }
                }
            }
        }
    }
}