INDEX_PARTS=parts

############
# Scout, ElasticSearch and Redis for Inventory Searchability AKA SDK
############
SCOUT_CHUNK_SEARCHABLE=500 # number of records to be sent to ElasticSearch as bulk (ingestion)
INDEX_INVENTORY=inventory # index alias

# due the inventory index must not be auto-create regarding special mappings, we need check if inventory is already create before send bulks
# this variable is only used when APP_ENV is `local` or `dev`, it should be moved to false once the developer has ran `inventory:recreate-index`
ELASTIC_SCOUT_DRIVER_CHECK_INDEX_INVENTORY=true

# see https://www.elastic.co/blog/how-many-shards-should-i-have-in-my-elasticsearch-cluster
# for production we recommend to have at least 1 shard per node
ELASTIC_SCOUT_DRIVER_INDEX_SETTINGS_INVENTORY_SHARDS=1
ELASTIC_SCOUT_DRIVER_INDEX_SETTINGS_INVENTORY_REPLICAS=1

ELASTIC_SCOUT_DRIVER_CACHE_TTL=28800 # 8 hours, basically this deppends on the Redis memory eviction policy
ELASTIC_SCOUT_DRIVER_COMPRESSION_LEVEL=9 # see https://www.php.net/manual/en/function.gzencode.php

# We most ensure `dealer-website` instance is also properly configured by using these DB numbers in `app/connections.ini`
# see `app/connections.ini.example` especifically `redis-api-search-cache` and `redis-api-single-cache`
REDIS_SDK_SEARCH_CACHE_DB=10 # used to store cache hits for search request (Search Result Pages)
REDIS_SDK_SINGLE_CACHE_DB=11 # used to store cache hits for single request (View Deatil Pages)

