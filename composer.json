{
    "name": "trailercentral/tc-api",
    "description": "TrailerCentral API",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.0",
        "ext-json": "*",
        "algolia/algoliasearch-client-php": "^2.4",
        "anaseqal/nova-import": "^0.0.6",
        "awobaz/compoships": "^2.1",
        "aws/aws-sdk-php": "^3.0",
        "babenkoivan/elastic-migrations": "^1.2",
        "babenkoivan/elastic-scout-driver": "^1.2",
        "babenkoivan/elastic-scout-driver-plus": "^1.11",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "barryvdh/laravel-snappy": "^0.4.8",
        "darkaonline/l5-swagger": "^6.0",
        "darkaonline/swagger-lume": "6.*",
        "dingo/api": "^2.4",
        "doctrine/dbal": "^2.9",
        "epartment/nova-dependency-container": "^1.2 <=1.3.2",
        "erusev/parsedown": "^1.7",
        "facebook/php-business-sdk": "5.0.5",
        "fideloper/proxy": "^4.0",
        "google/apiclient": "^2.7",
        "grimzy/laravel-mysql-spatial": "^1.0",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/container": "^6.0",
        "illuminate/contracts": "^6.0",
        "illuminate/mail": "^6.0",
        "illuminate/redis": "^6.3",
        "illuminate/support": "^6.0",
        "inspheric/nova-defaultable": "^1.2",
        "josiasmontag/laravel-redis-mock": "^1.3",
        "laravel/cashier": "^12.0",
        "laravel/framework": "^6.0",
        "laravel/horizon": "~3.0",
        "laravel/lumen-framework": "^6.0",
        "laravel/nova": "*",
        "laravel/scout": "^7.2",
        "laravel/tinker": "^2.0",
        "league/commonmark": "^1.3",
        "league/csv": "~9.7.4",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/html-to-markdown": "^5.1",
        "league/oauth2-client": "^2.6",
        "maatwebsite/laravel-nova-excel": "1.2.*",
        "maxbanton/cwh": "^2.0",
        "microsoft/microsoft-graph": "^1.34",
        "phenx/php-svg-lib": "0.3.3",
        "php-ews/php-ews": "~1.0",
        "pixel418/markdownify": "^2.3",
        "predis/predis": "^1.1",
        "propaganistas/laravel-phone": "4.2.7",
        "psr/cache": "~1.0",
        "psr/container": "1.1.1",
        "psr/log": "^1.0",
        "sentry/sentry-laravel": "^2.11",
        "showroom/bulkconfiguration": "*",
        "spatie/browsershot": "~3.40.0",
        "spatie/image": "^1.10",
        "spatie/laravel-permission": "^3.0",
        "spatie/laravel-sitemap": "^5.7",
        "spatie/ssl-certificate": "1.21.0",
        "staudenmeir/belongs-to-through": "^2.5",
        "stripe/stripe-php": "^7.100",
        "swiftmailer/swiftmailer": "^6.0",
        "symfony/css-selector": "^5.1",
        "symfony/dom-crawler": "^5.1",
        "tcentral/qbo-sdk": "^1.0",
        "tijsverkoyen/css-to-inline-styles": "^2.2.1",
        "trailercentral/passwordless-login-url": "*",
        "twilio/sdk": "^5.31",
        "vyuldashev/nova-permission": "^2.11",
        "webklex/laravel-imap": "^3.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "laravel/homestead": "^9.2",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0",
        "facade/ignition": "^1.4",
        "nunomaduro/collision": "^3.0",
        "brainmaestro/composer-git-hooks": "^2.8"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\traits\\": "database/traits",
            "Database\\helpers\\": "database/helpers"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "cghooks": "vendor/bin/cghooks",
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-install-cmd": "cghooks add --ignore-lock",
        "post-update-cmd": [
            "Google_Task_Composer::cleanup",
            "cghooks update"
        ],
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "discard-changes": true,
        "platform": {
            "ext-pcntl": "7.1",
            "ext-posix": "7.1"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/BulkConfiguration"
        },
        {
            "type": "path",
            "url": "./nova-components/PasswordlessLoginUrl"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:tcentral/qbo-sdk.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "google/apiclient-services": [
            "Gmail"
        ],
        "laravel": {
            "dont-discover": [
                "Grimzy\\LaravelMysqlSpatial\\SpatialServiceProvider",
                "sentry/sentry-laravel"
            ]
        },
        "hooks": {
            "pre-commit": [
                "@php artisan l5-swagger:generate"
            ]
        }
    }
}
